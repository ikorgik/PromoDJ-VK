{
  "name": "PromoDJ tools",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Sergey Korzh",
  "description": "Adds buttons to find song in VK and Yandex.music.",
  "homepage_url": "https://github.com/spleshka/SystemSeed-PT",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://promodj.com/*"
  ],
  "browser_action": {
    "default_title": "PromoDJ tools",
    "default_icon": "icons/icon32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://promodj.com/*"
      ],
      "js": [
        "src/main.js"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ]
}
